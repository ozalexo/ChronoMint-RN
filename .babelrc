{
  "presets": ["react-native"],
  "sourceMaps": true,
  "plugins": [
    "transform-decorators-legacy",
    "functional-hmr",
    "transform-new-target",
    "transform-exponentiation-operator",
    ["babel-plugin-transform-builtin-extend", {
      "globals": ["Error", "Array"]
    }],
    ["transform-inline-environment-variables", {
      "include": [
        "NODE_ENV",
        "REDUX_LOGGER"
      ]
    }]
  ]
}
